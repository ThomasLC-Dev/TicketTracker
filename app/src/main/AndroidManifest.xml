<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.thomas.tickettracker">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".users.NewUserActivity"></activity>
        <activity android:name=".users.UserDetailsActivity" />
        <activity android:name=".tickets.TicketDetailsTechActivity" />
        <activity android:name=".tickets.TicketDetailUserActivity" />
        <activity android:name=".tickets.TicketDetailsUserActivity" />
        <activity android:name=".tickets.NewTicketActivity" />
        <activity android:name=".home.TicketsListActivity" />
        <activity android:name=".home.HomeActivity" />
        <activity android:name=".connect.ConnectActivity">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN"
                    android:screenOrientation="portrait" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>